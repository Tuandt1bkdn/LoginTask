@model List<UserInfoModel>
@{
    ViewData["Title"] = "UserInfo";
}
<h1 class="m-4">VNPT Da Nang</h1>

<h3 class="m-5 text-black ">Danh sach nguoi dungggg</h3>

<table class="table" style="width:1000px; height:-600px; border:1px solid black; margin-left:auto; margin-right:auto;">
    <thead>
        <tr>
            <th scope="col">#</th>
            <th scope="col">Name</th>
            <th scope="col">Email</th>
            <th scope="col">BirthDay</th>
            <th scope="col">Location</th>
            <th scope="col">&#160;</th>
        </tr>    
    </thead>
    <tbody>
        @foreach(var user in Model)
        {
            <tr>
                <td>@user.UserId</td>
                <td>@user.Name</td>
                <td>@user.Email</td>
                <td>@user.Location</td>
            </tr>
        }
    </tbody>
    
</table>


